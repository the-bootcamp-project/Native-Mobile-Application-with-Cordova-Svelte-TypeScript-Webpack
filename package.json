	"browserslist": [
		"defaults"
	],
	"main": "./build/index.js",
	"scripts": {
		"cleaning": "********************************************************************************************************",
		"clean:cache": "yarn cache clean",
		"clean": "rm -rf node_modules build dist artifacts android temp coverage yarn.lock yarn*.log stats.json",
		"installation": "****************************************************************************************************",
		"add:android": "npx cap add android",
		"develop": "*********************************************************************************************************",
		"copy": "npx cap copy",
		"update": "npx cap update",
		"dev": "webpack serve --node-env development --mode development",
		"android:sync": "cross-env ANDROID_SDK_ROOT='$HOME/Android/Sdk/' CAPACITOR_ANDROID_STUDIO_PATH='/opt/android-studio-2020.3.1/android-studio/bin/studio.sh' npx cap sync",
		"android:run": "cross-env ANDROID_SDK_ROOT='$HOME/Android/Sdk/' CAPACITOR_ANDROID_STUDIO_PATH='/opt/android-studio-2020.3.1/android-studio/bin/studio.sh' npx cap run android --sdk-info",
		"android:open": "cross-env ANDROID_SDK_ROOT='$HOME/Android/Sdk/' CAPACITOR_ANDROID_STUDIO_PATH='/opt/android-studio-2020.3.1/android-studio/bin/studio.sh' npx cap open android",
		"linting": "*********************************************************************************************************",
		"bundling": "********************************************************************************************************",
		"testing": "*********************************************************************************************************",
		"building": "********************************************************************************************************",
		"build": "webpack --node-env production --mode production --progress --no-hot --no-watch",
		"build:stats": "webpack --node-env production --mode production --progress --no-hot --no-watch --profile --json > stats.json && webpack-bundle-analyzer stats.json",
		"versioning": "******************************************************************************************************"
	},
